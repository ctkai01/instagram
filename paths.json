{   
    "compilerOptions": {
        "baseUrl": "./src",
        "paths": {
            "@layouts/*": [
                "src/layouts/*"
            ],
            "@features/*": [
                "src/features/*"
            ],
            "@routes/*": [
                "src/routes/*"
            ],
            "@components/*": [
                "src/components/*"
            ],
            "@utils/*": [
                "src/utils/*"
            ],
            "@api/*": [
                "src/api/*"
            ],
            "@hooks/*": [
                "src/hooks/*"
            ],
            "@validations/*": [
                "src/validations/*"
            ],
            "@redux/*": [
                "src/redux/*"
            ],
            "@constants/*": [
                "src/constants/*"
            ],
            "@models/*": [
                "src/models/*"
            ]
        }
    }
}