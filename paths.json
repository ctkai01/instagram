{   
    "compilerOptions": {
        "baseUrl": "./src",
        "paths": {
            "@features": [
                "src/features"
            ],
            "@routes": [
                "src/routes"
            ],
            "@components": [
                "src/components"
            ],
            "@utils/*": [
                "src/utils/*"
            ],
            "@api": [
                "src/api"
            ],
            "@hooks": [
                "src/hooks"
            ],
            "@validations": [
                "src/validations"
            ],
            "@app": [
                "src/app"
            ],
            "@constants": [
                "src/constants"
            ],
            "@models": [
                "src/models"
            ]
        }
    }
}